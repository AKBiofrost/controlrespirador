        <activity android:name=".InformacionRespirador" />
        <activity android:name=".Frecuencia" />
        <activity android:name=".Menu" />
        <activity android:name=".SincronizarBluetooth" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>